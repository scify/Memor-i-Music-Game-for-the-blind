<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.StackPane?>
<StackPane alignment="center" styleClass="rootPane" xmlns="http://javafx.com/javafx/8.0.102-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.scify.windmusicgame.FXHighScoresScreenController">
    <HBox alignment="BOTTOM_RIGHT" spacing="5" styleClass="logoContainer">
        <ImageView styleClass="boxImage">
            <image>
                <Image requestedHeight="250" requestedWidth="250" url="@../img/memori.png" />
            </image>
        </ImageView>
    </HBox>
    <VBox alignment="center" styleClass="optionButtonContainer">
        <Button contentDisplay="CENTER" styleClass="optionButton" text="2x3" fx:id="level1" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="2x4" fx:id="level2" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="3x4" fx:id="level3" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="4x4" fx:id="level4" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="5x4" fx:id="level5" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="4x6" fx:id="level6" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="5x6" fx:id="level7" onKeyPressed="#parseHighScore"/>
        <Button contentDisplay="CENTER" styleClass="optionButton" text="Πίσω" fx:id="back" onKeyPressed="#backToMainScreen"/>
    </VBox>

    <stylesheets>
        <URL value="@/css/style.css" />
    </stylesheets>
</StackPane>
